{
    "name": "lnw/core",
    "description": "Core package for Lnw",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/validation": "^8.2",
        "illuminate/database": "^8.2",
        "illuminate/filesystem": "^8.2",
        "illuminate/translation": "^8.2",
        "illuminate/events": "^8.2",
        "vlucas/phpdotenv": "^5.2.0",
        "illuminate/support": "^8.2",
        "nikic/fast-route": "^1.3",
        "phpoption/phpoption": "^1.7"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Lnw\\Core\\": "Classes/"
        },
        "files": [
            "Classes/helpers.php"
        ],
        "classmap": [{
            "Msg": "Classes/Messages.php",
            "Config": "Classes/Config.php",
            "Datatables": "Classes/Datatables.php",
            "Token": "Classes/Token.php"
        }]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "license": "MIT",
    "authors": [{
        "name": "Channarong Phonkhan",
        "email": "channaxx@gmail.com"
    }],
    "prefer-stable": true
}